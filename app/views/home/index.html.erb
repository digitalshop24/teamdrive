  <script type="text/ng-template" id="eventModal">
    <div class="eventsModal" ng-controller="slickCtrl">
      <div class="container" ng-repeat="terrapod in terrapods | filter:{id:thisID} " >
        <div class="row">
          <div class="col-md-offset-2 col-md-8 text-center">
            <h2>{{terrapod.title}}</h2>
          </div>
        </div>
        <div class="row" >
          <div class="col-md-offset-2 col-md-8">
            <slick init-onload="true"data="terrapods" slides-to-show="1"slides-to-scroll="1"arrows="true" dots="false" class="text-center"responsive="breakpoints">
            <div class="item" ng-repeat="slide in terrapod.images">
              <div class="itemWrap">
                <div class="contentItem" style="background-image:url('{{slide}}');"></div>
                </div>
            </div>
          </slick>
          </div>
        </div>
        <div class="row">
          <div class="col-md-offset-2 col-md-8">
            <div class="fullCont" ng-bind-html="terrapod.body"></div>
          </div>
        </div>
      </div>
    </div>
  </script>



